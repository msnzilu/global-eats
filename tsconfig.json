{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "lib": [
      "ES2020"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "nativewind-global-types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}